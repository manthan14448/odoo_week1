<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_doctor_detail_tag" model="ir.actions.act_window">
        <field name="name">Doctor_Detail view</field>
        <field name="res_model">doctor.details</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="doctor_details_view_tree" model="ir.ui.view">
        <field name="name">doctor_details.view.tree</field>
        <field name="model">doctor.details</field>
        <field name="arch" type="xml">
            <tree string="doctor">
                <field name="number" />
                <field name="name" />
                <field name="speciality" />
                <field name="degree_info" widget="many2many_tags" />
            </tree>

        </field>
    </record>
    <record id="doctor_details_view_form" model="ir.ui.view">
        <field name="name">doctor_details.view.form</field>
        <field name="model">doctor.details</field>
        <field name="arch" type="xml">
            <form string="Docter Detail">
                <sheet>
                    <group>
                        <field name="number" />
                        <field name="name" />
                        <field name="speciality" />
                        <field name="degree_info" widget="many2many_tags"></field>
                    </group>
                </sheet>
            </form>

        </field>
    </record>
</odoo>