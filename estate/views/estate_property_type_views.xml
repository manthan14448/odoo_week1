<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_estate_property_type" model="ir.actions.act_window">
        <field name="name">Estate_property_Type view</field>
        <field name="res_model">estate.property.type</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="estate_property_type_form_tree" model="ir.ui.view">
        <field name="name">estate_property_type.form.tree</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1 style="color:red;">
                        <field name="sequence" widget="handle" />
                        <field name="name" string="Property Type" />
                        <hr />
                    </h1>
                    <notebook>
                        <page name="properties" string="Properties">
                            <field name="property_ids">
                                <tree>
                                    <field name="name" string="Title" />
                                    <field name="expected_price" string="Expected Price" />
                                    <field name="status" string="Status" />
                                </tree>
                            </field>

                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>